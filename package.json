    "changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./src/index.js"
      },
      "require": {
        "default": "./src/index.js"
      }
    },
    "./bin/celeris": "./bin/celeris.js"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "src"
  ],
  "bin": {
    "celeris": "./bin/celeris.js"
  },
  "engines": {
    "node": ">=18"
  },
